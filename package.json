{
  "name": "react-redux-notify",
  "version": "0.0.1",
  "description": "A notifications component built for use with React and Redux.",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:umd": "webpack src/index.js dist/ReactReduxNotify.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/ReactReduxNotify.min.js",    
    "build:lib": "babel src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist lib",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "react-component",
    "notifications",
    "react",
    "react notifications",
    "reactjs",
    "redux notifications"
  ],
  "author": "Deeptesh Chagan",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.9.3",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-css-modules": "^3.7.10",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6"
  },
  "peerDependencies": {
    "react": ">= 15.0.0 < 16.0.0",
    "react-redux": ">= 4.0.0 < 6.0.0",
    "redux": ">= 3.0.0 < 4.0.0"
  }  
}
